{
  "expo": {
    "name": "mushroomonitor",
    "slug": "mushroomonitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-main.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-main.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"

    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-main.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  },
  "scheme": "msh://mushroomonitor"
}
